import java.util.Scanner;

class Student{
	String name;
    int age;
    public void nhap(){
        Scanner sc = new Scanner(System.in);
        name = sc.next();
        age = sc.nextInt();
    }
    public void xuat(){
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}


public class Entry {
	public static void main(String[]args) {
		Student st = new Student();
		st.nhap();
		st.xuat();
	}
}

Input:
- Khanh
- 23
Output
- Name: Khanh
- Age: 23
